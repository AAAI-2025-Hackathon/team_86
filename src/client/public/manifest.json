{
    "manifest_version": 3,
    "name": "AI Web Assistant",
    "version": "1.0.0",
    "description": "An AI-powered web assistant",
    "permissions": ["activeTab", "scripting", "storage", "tabs"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }